<%= form_for(search) do |f| %>
  Search: <%= f.text_field(:str) %>
  <%= f.hidden_field(:class_name) %>
  <%= f.submit("Search") %>
  <% unless search.new_record? %>
    <%= f.submit("Clear", :onclick => "this.form.search_search_str.value=''") %>
  <% end %>
  <br/>
  <%= search_examples(search) %>
<% end %>
