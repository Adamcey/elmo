    <input ref="/data/untitled7">
      <label ref="jr:itext('/data/untitled7:label')"/>
      <hint ref="jr:itext('/data/untitled7:hint')"/>
    </input>
    <select1 ref="/data/untitled8">
      <label ref="jr:itext('/data/untitled8:label')"/>
      <hint ref="jr:itext('/data/untitled8:hint')"/>
    </select1>
    <select ref="/data/untitled9">
      <label ref="jr:itext('/data/untitled9:label')"/>
      <hint ref="jr:itext('/data/untitled9:hint')"/>
