          <text id="/data/untitled9:label">
            <value>cappy6</value>
          </text>
          <text id="/data/untitled9:hint">
            <value/>
          </text>
        </translation>
      </itext>
      <bind nodeset="/data/untitled4" type="string"/>
      <bind nodeset="/data/untitled5" type="int" required="true()"/>
      <bind nodeset="/data/untitled6" type="date" required="true()"/>
      <bind nodeset="/data/untitled7" type="geopoint"/>
      <bind nodeset="/data/untitled8" type="select1"/>
      <bind nodeset="/data/untitled9" type="select"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/untitled4">
      <label ref="jr:itext('/data/untitled4:label')"/>
