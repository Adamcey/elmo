          <untitled6/>
          <untitled7/>
          <untitled8/>
          <untitled9/>
        </data>
      </instance>
      <itext>
        <translation lang="eng">
          <text id="/data/untitled4:label">
            <value>cappy</value>
          </text>
          <text id="/data/untitled4:hint">
            <value/>
          </text>
          <text id="/data/untitled5:label">
            <value>cappy2</value>
