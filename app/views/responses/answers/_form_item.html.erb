<%# Renders the answer sets for a form item, be it a group or a questioning. %>
<% @response.answer_groups %>
<% if item.is_a?(QingGroup) %>
  <%# No point rendering an empty group. %>
  <% unless subtree.empty? %>
    <%= render 'responses/answers/qing_group', item: item %>
  <% end %>
<% elsif !item.hidden? %>
  <%= render 'responses/answers/answer_set',
        answer_set: @response.answer_groups.answer_set_for_questioning(item),
        inst_num: nil %>
<% end %>
